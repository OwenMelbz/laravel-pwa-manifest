{
    "name": "owenmelbz/laravel-pwa-manifest",
    "description": "Laravel 5.5+ helper to generate basic PWA style manifests.",
    "keywords": ["laravel", "pwa", "manifest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Owen Melbourne",
            "email": "owenmelbz@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.*",
        "intervention/imagecache": "2.*"
    },
    "autoload": {
        "psr-4": {
            "OwenMelbz\\PwaManifest\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwenMelbz\\PwaManifest\\PwaManifestServiceProvider"
            ]
        }
    }
}
